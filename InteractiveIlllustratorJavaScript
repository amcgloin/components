            $("#column11").mouseout(function() {
                $("#column11").fadeTo(1000, 1);
                $("#numbers11").fadeTo(1000, 0);
                $("#column11").backgroundColor('#5E9F59')
            });
            
            //**Twelth Column
            
            //**Fades out first column, fades in number on mouseenter
            $("#column12").mouseenter(function() {
                $("#column12").fadeTo(1000, 0.4);
                $("#numbers12").fadeTo(1000, 1);
                $("#column12").backgroundColor('#5E9F59')
            });
            //**Fades back first column, fades out number on exit
            $("#column12").mouseout(function() {
                $("#column12").fadeTo(1000, 1);
                $("#numbers12").fadeTo(1000, 0);
                $("#column12").backgroundColor('#5E9F59')
            });
            
            //**Thirteenth Column
            
            //**Fades out thirteenth column, fades in number on mouseenter
            $("#column13").mouseenter(function() {
                $("#column13").fadeTo(1000, 0.4);
                $("#numbers13").fadeTo(1000, 1);
