            $("#column7").mouseout(function() {
                $("#column7").fadeTo(1000, 1);
                $("#numbers7").fadeTo(1000, 0);
                $("#column7").backgroundColor('#5E9F59')
            });
            
            //**Eigth Column
            
            //**Fades out eigth column, fades in number on mouseenter
            $("#column8").mouseenter(function() {
                $("#column8").fadeTo(1000, 0.4);
                $("#numbers8").fadeTo(1000, 1);
                $("#column8").backgroundColor('#5E9F59')
            });
            //**Fades back eigth column, fades out number on exit
            $("#column8").mouseout(function() {
                $("#column8").fadeTo(1000, 1);
                $("#numbers8").fadeTo(1000, 0);
                $("#column8").backgroundColor('#5E9F59')
            });
            
            
            //**Ninth Column
            
            //**Fades out nith column, fades in number on mouseenter
            $("#column9").mouseenter(function() {
                $("#column9").fadeTo(1000, 0.4);
                $("#numbers9").fadeTo(1000, 1);
                $("#column9").backgroundColor('#5E9F59')
            });
            //**Fades back ninth column, fades out number on exit
            $("#column9").mouseout(function() {
                $("#column9").fadeTo(1000, 1);
                $("#numbers9").fadeTo(1000, 0);
                $("#column9").backgroundColor('#5E9F59')
            });
            
            //**Tenth Column
            
            //**Fades out tenth column, fades in number on mouseenter
            $("#column10").mouseenter(function() {
