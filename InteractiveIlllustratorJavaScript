            $("#numbers13").fadeTo(1000, 0.0);
            
            //**Fades text boxes on load of graph
            $("#textbox1").fadeTo(1000, 0.0);
            $("#textbox2").fadeTo(1000, 0.0);
            
            
            //**Fades lines connecting to text boxes on load of graph
            $("#line1").fadeTo(1000, 0.0);
            $("#line2").fadeTo(1000, 0.0);
            
            
            //**First Column
            
            //**Fades out first column, fades in number on mouseenter
            $("#1rect").mouseenter(function() {
                $("#1rect").fadeTo(1000, 0.4);
                $("#numbers1").fadeTo(1000, 1);
            });
            //**Fades back first column, fades out number on exit
            $("#1rect").mouseout(function() {
                $("#1rect").fadeTo(1000, 1);
                $("#numbers1").fadeTo(1000, 0);
            });
            
            //**Second Column
            //**Text boxes
            
            //**Fades out second column, fades in number on mouseenter
            $("#2rect").mouseenter(function() {
                $("#2rect").fadeTo(1000, 0.4);
                $("#textbox1").fadeTo(1000, 1);
                $("#line1").fadeTo(1000, 1);
                $("#numbers2").fadeTo(1000, 1);
            });
             //**Fades back second column, fades out number on exit
            $("#2rect").mouseout(function() {
                $("#2rect").fadeTo(1000, 1);
                $("#textbox1").fadeTo(1000, 0);
                $("#line1").fadeTo(1000, 0);
                $("#numbers2").fadeTo(1000, 0);
            });
            
