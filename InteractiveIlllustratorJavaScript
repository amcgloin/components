 //**To apply to an SVG,  apply the selectors to the applicable text boxes, rectangles, or lines.
 //**The color change is optional, and is currently set at green, so there's no change. If color change is preferred, change the hex code on mouseenter to desired color

//**ie:
//**<rect x="187" y="283.6" class="st4" width="35.4" height="504.9"/>
//**Turns into 
//**<rect id="rect1fade"  x="187" y="283.6" class="st4" width="35.4" height="504.9"/>
 
 
 <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script>
     //**Fade effect on for text boxes on hover
        $(document).ready(function() {
            //**Fades all numbers on load of graph
            $("#numbers1").fadeTo(1000, 0.0);
            $("#numbers2").fadeTo(1000, 0.0);
            $("#numbers3").fadeTo(1000, 0.0);
            $("#numbers4").fadeTo(1000, 0.0);
            $("#numbers5").fadeTo(1000, 0.0);
            $("#numbers6").fadeTo(1000, 0.0);
            $("#numbers7").fadeTo(1000, 0.0);
            $("#numbers8").fadeTo(1000, 0.0);
            $("#numbers9").fadeTo(1000, 0.0);
            $("#numbers10").fadeTo(1000, 0.0);
            $("#numbers11").fadeTo(1000, 0.0);
            $("#numbers12").fadeTo(1000, 0.0);
            $("#numbers13").fadeTo(1000, 0.0);
            
            //**Fades text boxes on load of graph
            $("#textbox1").fadeTo(1000, 0.0);
            $("#textbox2").fadeTo(1000, 0.0);
            
            
            //**Fades lines connecting to text boxes on load of graph
            $("#line1").fadeTo(1000, 0.0);
            $("#line2").fadeTo(1000, 0.0);
            
            
            //**First column
            
            //**Fades out first column, fades in number on mouseenter
            $("#column1").mouseenter(function() {
                $("#column1").fadeTo(1000, 0.4);
                $("#numbers1").fadeTo(1000, 1);
                $("#column1").backgroundColor('#5E9F59')
            });
            //**Fades back first column, fades out number on exit
            $("#column1").mouseout(function() {
                $("#column1").fadeTo(1000, 1);
                $("#numbers1").fadeTo(1000, 0);
                $("#column1").backgroundColor('#5E9F59')
            });
            
            //**Second column
            //**Text boxes
            
            //**Fades out second column, fades in number on mouseenter
            $("#column2").mouseenter(function() {
                $("#column2").fadeTo(1000, 0.4);
                $("#textbox1").fadeTo(1000, 1);
                $("#line1").fadeTo(1000, 1);
                $("#numbers2").fadeTo(1000, 1);
                $("#column2").backgroundColor('#5E9F59')
            });
             //**Fades back second column, fades out number on exit
            $("#column2").mouseout(function() {
                $("#column2").fadeTo(1000, 1);
                $("#textbox1").fadeTo(1000, 0);
                $("#line1").fadeTo(1000, 0);
                $("#numbers2").fadeTo(1000, 0);
                $("#column2").backgroundColor('#5E9F59')
            });
            
            //**Third Column
            
            //**Fades out third column, fades in number on mouseenter
            $("#column3").mouseenter(function() {
                $("#column3").fadeTo(1000, 0.4);
                $("#numbers3").fadeTo(1000, 1);
                $("#column3").backgroundColor('#5E9F59')
            });
            //**Fades back third column, fades out number on exit
            $("#column3").mouseout(function() {
                $("#column3").fadeTo(1000, 1);
                $("#numbers3").fadeTo(1000, 0);
                $("#column3").backgroundColor('#5E9F59')
            });
            
            //**Fourth Column
            
            //**Fades out fourth column, fades in number on mouseenter
            $("#column4").mouseenter(function() {
                $("#column4").fadeTo(1000, 0.4);
                $("#numbers4").fadeTo(1000, 1);
                $("#column4").backgroundColor('#5E9F59')
            });
            //**Fades back fourth column, fades out number on exit
            $("#column4").mouseout(function() {
                $("#column4").fadeTo(1000, 1);
                $("#numbers4").fadeTo(1000, 0);
                $("#column4").backgroundColor('#5E9F59')
            });
            
            //**Fifth Column
            
            //**Fades out fifth column, fades in number on mouseenter
            $("#column5").mouseenter(function() {
                $("#column5").fadeTo(1000, 0.4);
                $("#numbers5").fadeTo(1000, 1);
                $("#column5").backgroundColor('#5E9F59')
            });
            //**Fades back fifth column, fades out number on exit
            $("#column5").mouseout(function() {
                $("#column5").fadeTo(1000, 1);
                $("#numbers5").fadeTo(1000, 0);
                $("#column5").backgroundColor('#5E9F59')
            });
            
            //**Sixth Column
            
            //**Fades out sixth column, fades in number on mouseenter
            $("#column6").mouseenter(function() {
                $("#column6").fadeTo(1000, 0.4);
                $("#numbers6").fadeTo(1000, 1);
                $("#column6").backgroundColor('#5E9F59')
            });
            //**Fades back sixth column, fades out number on exit
            $("#column6").mouseout(function() {
                $("#column6").fadeTo(1000, 1);
                $("#numbers6").fadeTo(1000, 0);
                $("#column6").backgroundColor('#5E9F59')
            });
            
            //**Seventh Column
            
