 <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script>
        $(document).ready(function() {
            //**Fades all numbers on load of graph
            $("#numbers1").fadeTo(1000, 0.0);
            $("#numbers2").fadeTo(1000, 0.0);
            $("#numbers3").fadeTo(1000, 0.0);
            $("#numbers4").fadeTo(1000, 0.0);
            $("#numbers5").fadeTo(1000, 0.0);
            $("#numbers6").fadeTo(1000, 0.0);
            $("#numbers7").fadeTo(1000, 0.0);
            $("#numbers8").fadeTo(1000, 0.0);
