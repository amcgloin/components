                $("#numbers10").fadeTo(1000, 1);
                $("#column10").backgroundColor('#5E9F59')
            });
            //**Fades back tenth column, fades out number on exit
            $("#column10").mouseout(function() {
                $("#column10").fadeTo(1000, 1);
                $("#numbers10").fadeTo(1000, 0);
                $("#column10").backgroundColor('#5E9F59')
            });
            
            //**Eleventh Column
            
